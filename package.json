{
  "name": "tolgee-cli",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "A tool to interact with the Tolgee Platform through CLI",
  "bin": {
    "tolgee-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write ./src",
    "run-dev": "npm run build && node --enable-source-maps ./dist/index.js",
    "schema": "openapi-typescript https://app.tolgee.io/v3/api-docs/V2%20Accessible%20with%20API%20key%20V2 --output src/client/schema.generated.ts"
  },
  "author": "Jan Cizmar",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.0",
    "form-data": "^4.0.0",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "openapi-typescript": "^5.4.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
