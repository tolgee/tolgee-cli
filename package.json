{
  "name": "tolgee-cli",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "A tool to interact with the Tolgee Platform through CLI",
  "bin": {
    "tolgee": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "lint": "prettier --check ./src ./test",
    "prettier": "prettier --write ./src ./test",
    "run-dev": "ts-node ./src/index.ts",
    "schema": "openapi-typescript https://app.tolgee.io/v3/api-docs/V2%20Accessible%20with%20API%20key%20V2 --output src/client/internal/schema.generated.ts"
  },
  "author": "Jan Cizmar",
  "license": "MIT",
  "dependencies": {
    "base32-decode": "^1.0.0",
    "commander": "^9.4.0",
    "cosmiconfig": "^7.0.1",
    "form-data": "^4.0.0",
    "glob": "^8.0.3",
    "undici": "^5.10.0",
    "vscode-oniguruma": "^1.6.2",
    "vscode-textmate": "^7.0.3",
    "xstate": "^4.34.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.7.18",
    "@types/yauzl": "^2.10.0",
    "jest": "^29.2.1",
    "openapi-typescript": "^5.4.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
